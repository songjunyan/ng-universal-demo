{
    "compilerOptions":
    {
        "target": "es5",
        "module": "es2015",
        "sourceMap": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "moduleResolution": "node",
        "outDir": "wwwroot/app",
        "lib": 
        [
            "es2016",
            "dom"
        ],
        "types": 
        [
            "moment",
            "node",
            "systemjs",
            "jasmine"
        ],
        "typeRoots": 
        [
            "node_modules/@types"
        ],
        "plugins": 
        [
            {
                "name": "@angular/language-service"
            }
        ]
    },
    "exclude": 
    [
        "node_modules",
        "app/main.server.aot.ts"
    ]
}